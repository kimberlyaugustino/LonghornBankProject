@model IEnumerable<LonghornBankProject.Models.StockQuote>

@{
    ViewBag.Title = "StockQuotes";
}

<h2>StockQuotes</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Symbol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PreviousClose)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastTradePrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Volume)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Symbol)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PreviousClose)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastTradePrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Volume)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StockQuoteID }) |
            @Html.ActionLink("Details", "Details", new { id=item.StockQuoteID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StockQuoteID })
        </td>
    </tr>
}

</table>
